{
  "name": "ts-x-unit",
  "repository": {
    "type": "git",
    "url": "git@github.com:FreeElephants/TSxUnit.git"
  },
  "description": "xUnit for TypeScript projects",
  "keywords": [
    "xUnit",
    "ts",
    "typescript"
  ],
  "author": "samizdam",
  "license": "BSD-2-Clause",
  "scripts": {
    "tsc": "tsc",
    "typings": "typings",
    "instanbul": "instanbul",
    "remap-istanbul": "remap-istanbul",
    "build": "tsc @tsxunit-build.config",
    "build-launch-builder": "tsc @tsxunit-launch-builder.config",
    "pretest": "npm run build-launch-builder && node bin/launch-builder.js tests/ && tsc -p tests/",
    "test": "node tests/tests.js",
    "coverage": "istanbul cover tests/test.js && remap-istanbul --input coverage/coverage.json --output coverage/html-report --type html"
  },
  "bin": {
    "launch-builder": "bin/launch-builder"
  },
  "main": "build/index.js",
  "typescript": {
    "definition": "build/index.d.ts"
  },
  "dependencies": {
    "istanbul": "^0.4.2",
    "remap-istanbul": "^0.5.1",
    "typescript": "1.8.9",
    "typings": "0.7.9"
  },
  "version": "0.2.0",
  "devDependencies": {
    "tslint": "^3.7.1"
  }
}
