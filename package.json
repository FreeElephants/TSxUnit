{
  "name": "ts-x-unit",
  "version": "0.0.1",
  "repository": "https://gitlab.com/FreeElephants/TSxUnit",
  "description": "xUnit for TypeScript projects",
  "scripts": {
    "tsc": "tsc",
    "typings": "typings",
    "postinstall": "typings install",
    "build": "tsc @tsxunit-build.config",
    "pretest": "tsc @tsxunit-launch-builder.config && node bin/launch-builder.js tests/ && tsc",
    "test": "node out.js"
  },
  "main": "build/index.js",
  "typescript": {
    "definition": "build/index.d.ts"
  },
  "keywords": [
    "xUnit",
    "ts",
    "typescript"
  ],
  "author": "samizdam",
  "license": "BSD-2-Clause",
  "dependencies": {
    "typescript": "^1.8.9",
    "typings": "^0.7.9"
  }
}
