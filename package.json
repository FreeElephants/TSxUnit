{
  "name": "ts-x-unit",
  "repository": {
    "type": "git",
    "url": "git@github.com:FreeElephants/TSxUnit.git"
  },
  "description": "xUnit for TypeScript projects",
  "keywords": [
    "xUnit",
    "ts",
    "typescript"
  ],
  "author": "samizdam",
  "license": "BSD-2-Clause",
  "scripts": {
    "tsc": "tsc",
    "typings": "typings",

    "build": "tsc @tsxunit-build.config",

    "build-launch-builder": "tsc @tsxunit-launch-builder.config",

    "pretest": "npm run build-launch-builder && node bin/launch-builder.js tests/ && tsc",
    "test": "node out.js"
  },
  "bin": {
    "launch-builder": "bin/launch-builder"
  },
  "main": "build/index.js",
  "typescript": {
    "definition": "build/index.d.ts"
  },
  "dependencies": {
    "typescript": "1.8.9",
    "typings": "0.7.9"
  },
  "version": "0.0.6-rc.4"
}
